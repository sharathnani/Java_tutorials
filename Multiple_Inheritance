class Multiple_Inheritance
{
  BabyDog d=new BabyDog();
  d.eat();
  d.bark();
  d.sleep();
}
class Animal
{
  public void eat()
  {
    System.out.println("eating");
  }
}
class Dog extends Animal
{
  public void bark()
  {
    System.out.println("barking");
  }
}
class BabyDog extends Dog
{
  public void sleep()
  {
    System.out.println("sleeping");
  }
}
